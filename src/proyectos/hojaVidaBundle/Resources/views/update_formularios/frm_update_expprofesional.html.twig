                function sum_lic_otor() {
                    var sum = 0;
                    for (var i = 0; i < $(".txt_dias_lic_otor").size(); i++) {
                        if ($(".txt_dias_lic_otor").eq(i).val() === "") {
                            $(".txt_dias_lic_otor").eq(i).val(0);
